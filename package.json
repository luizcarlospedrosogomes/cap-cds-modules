{
  "name": "cap-cds-modules-api",
  "version": "1.0.0",
  "description": "Generated by cds init",
  "repository": "<Add your repository here>",
  "license": "ISC",
  "files": [
    "srv"
  ],
  "dependencies": {
    "@dxfrontier/cds-ts-dispatcher": "^0.1.24",
    "@dynatrace/oneagent-sdk": "^1.5.0",
    "@sap-cloud-sdk/connectivity": "^3.11.0",
    "@sap-cloud-sdk/http-client": "^3.11.0",
    "@sap/cds": "^7.5.3",
    "@sap/cds-compiler": "^4.5.0",
    "@sap/cds-hana": "^2.0.0",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/hana-client": "^2.19",
    "@sap/hdbext": "^8.0.3",
    "@sap/jobs-client": "^1.8.3",
    "@sap/logging": "^7.1.0",
    "@sap/xsenv": "^4.2.0",
    "@sap/xssec": "^3.6.1",
    "body-parser": "^1.20.2",
    "cds-swagger-ui-express": "^0.8.0",
    "cf-http-client": "^1.3.0",
    "cf-nodejs-logging-support": "^7.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-status-monitor": "^1.3.4",
    "hdb": "^0.19.7",
    "helmet": "^7.1.0",
    "http-auth": "^4.2.0",
    "http-auth-connect": "^1.0.6",
    "mysql": "^2.18.1",
    "odata2openapi": "^1.3.2",
    "passport": "^0.7.0",
    "require-dir": "^1.2.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "scripts": {
    "start:prd": "cds watch --profile production",
    "start": "cds run",
    "dev": "cds-ts watch",
    "deploy": "cds deploy",
    "build": "cds build --clean",
    "build:cf": "mbt build -p=cf",
    "test": "jest --coverage --forceExit --detectOpenHandles --silent",
    "jest": "jest ",
    "test-bind": "cds bind --profile development --exec npm run jest",
    "bind:dev": "cds bind --profile development"
  },
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "sqlite",
          "impl": "@cap-js/sqlite",
          "credentials": {
            "url": "memory"
          }
        },
        "[production]": {
          "kind": "hana",
          "impl": "@sap/cds-hana",
          "deploy-format": "hdbtable"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable",
      "syntax": "hdi"
    },
    "build": {
      "target": "gen",
      "tasks": [
        {
          "src": "srv",
          "for": "hana",
          "options": {
            "model":["srv/index.cds"]
          }
        },
        {
          "src": "srv",
          "for": "nodejs",
          "options": {
            "model":["srv/index.cds"]
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.2.0",
    "@cap-js/sqlite": "^1.4.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.9",
    "@types/passport": "^1.0.16",
    "@types/require-dir": "^1.0.4",
    "@types/sap__xsenv": "^3.3.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "axios": "^1.6.7",
    "chai": "2.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-stare": "^2.5.1",
    "jest-tesults-reporter": "^1.2.3",
    "jest-trx-results-processor": "^3.0.2",
    "nodemon": "^3.0.3",
    "odata-batch-request": "^0.6.2",
    "sqlite3": "5.1.7",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "json",
      "html"
    ],
    "coverageDirectory": "./coverage",
    "reporters": [
      "default",
      [
        "jest-stare",
        {
          "resultDir": "results/jest-stare",
          "reportTitle": "jest-stare!",
          "additionalResultsProcessors": [
            "jest-junit"
          ],
          "coverageLink": "../../coverage/lcov-report/index.html",
          "jestStareConfigJson": "jest-stare.json",
          "jestGlobalConfigJson": "globalStuff.json"
        }
      ],
      [
        "jest-html-reporters",
        {
          "publicPath": "./coverage",
          "filename": "report.html",
          "openReport": true
        }
      ],
      [
        "jest-trx-results-processor",
        {
          "outputFile": "./coverage/jestTestresults.trx",
          "defaultUserName": "user name to use if automatic detection fails"
        }
      ]
    ]
  }
}
